# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_API_TIMEOUT=30000

# App Configuration
REACT_APP_NAME=MatchCare
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Features Toggle
REACT_APP_ENABLE_GUEST_MODE=true
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_ANALYTICS=false

# Cache Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_ENABLE_OFFLINE_MODE=true

# UI Configuration
REACT_APP_THEME_COLOR=#3B82F6
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,id

# Image Configuration
REACT_APP_IMAGE_PLACEHOLDER=/images/placeholders/product-placeholder.svg
REACT_APP_IMAGE_QUALITY=medium
REACT_APP_LAZY_LOADING=true

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_MIXPANEL_TOKEN=
REACT_APP_HOTJAR_ID=

# Social Media (for sharing)
REACT_APP_FACEBOOK_APP_ID=
REACT_APP_TWITTER_HANDLE=@matchcare
REACT_APP_INSTAGRAM_HANDLE=@matchcare

# SEO Configuration
REACT_APP_SITE_URL=http://localhost:3000
REACT_APP_SITE_DESCRIPTION=Discover your perfect skincare match with our ontology-based recommendation system
REACT_APP_SITE_KEYWORDS=skincare,recommendation,ontology,beauty,skin analysis

# Error Tracking
REACT_APP_SENTRY_DSN=

# Performance Monitoring
REACT_APP_PERFORMANCE_MONITORING=false
REACT_APP_WEB_VITALS_ENDPOINT=

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_QUERY_DEVTOOLS=true
REACT_APP_SHOW_DEBUG_INFO=false

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_SECURE_COOKIES=false

# Experimental Features
REACT_APP_ENABLE_VOICE_SEARCH=false
REACT_APP_ENABLE_AR_TRY_ON=false
REACT_APP_ENABLE_AI_CHAT=false

# Localization
REACT_APP_DEFAULT_CURRENCY=IDR
REACT_APP_DEFAULT_TIMEZONE=Asia/Jakarta

# External Services
REACT_APP_MAPS_API_KEY=
REACT_APP_STRIPE_PUBLIC_KEY=
REACT_APP_PAYPAL_CLIENT_ID=

# Brand Configuration
REACT_APP_BRAND_COLOR_PRIMARY=#3B82F6
REACT_APP_BRAND_COLOR_SECONDARY=#8B5CF6
REACT_APP_BRAND_FONT_FAMILY=Inter